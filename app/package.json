  "license": "ISC",
  "repository": "git@github.com:gyamin/starter-AngularTypeScript.git",
  "devDependencies": {},
  "dependencies": {
    "bower": "^1.7.9",
    "node-sass": "^3.9.3",
    "typescript": "^1.8.10",
    "typings": "^1.3.2"
  }
}
