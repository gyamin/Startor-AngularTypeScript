{
  "name": "app",
  "version": "1.0.0",
  "description": "App for Angularjs and TypeScript.",
  "main": "index.js",
  "scripts": {
    "clean": "bin/clean.sh",
    "build:bower":  "bin/build_bower.sh $npm_package_config_postfix",
    "build:ts":     "bin/build_ts.sh",
    "watch:ts":     "bin/build_ts.sh watch",
    "build:sass":   "bin/build_sass.sh",
    "watch:sass":   "bin/build_sass.sh watch",
    "build:all":    "npm run build:bower & npm run build:ts & npm run build:sass",
    "watch:all":    "npm run watch:ts & npm run watch:sass"
  },
  "config": {
    "postfix": ".min"
  },
